DEPTH     = ../../../../../..
topsrcdir = @top_srcdir@
srcdir    = @srcdir@
VPATH     = @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE = pagespeed
DIRS = gif minitiff pnm

LIBRARY_NAME = pngxtern_s
USE_STATIC_LIBS = 1
FORCE_STATIC_LIB = 1

CSRCS =                      \
    gif/gifread.c            \
    minitiff/minitiff.c      \
    minitiff/tiffread.c      \
    minitiff/tiffwrite.c     \
    pngxio.c                 \
    pngxmem.c                \
    pngxrbmp.c               \
    pngxread.c               \
    pngxrgif.c               \
    pngxrjpg.c               \
    pngxrpnm.c               \
    pngxrtif.c               \
    pngxset.c                \
    pngxwrite.c              \
    pnm/pnmin.c              \
    pnm/pnmout.c             \
    pnm/pnmutil.c            \
    $(NULL)

LOCAL_INCLUDES	= -I$(srcdir)
LOCAL_INCLUDES += -I$(srcdir)/../zlib
LOCAL_INCLUDES += -I$(srcdir)/../libpng

include $(topsrcdir)/config/rules.mk
